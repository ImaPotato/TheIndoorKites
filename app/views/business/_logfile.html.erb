<div>
  <table>
    <thead>
      <tr>
        <th>Event Details</th>
        <th>Time</th>
      </tr>
    </thead>

    <tbody>
      <% @events.each do |event| %>
        <tr>
          <td><%= event.event_details %></td>
          <td><%= event.event_time %></td>
        </tr>
      <% end %>
    </tbody>
  </table>
</div>